# Скрипт парсинга
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_PHONE=
TELEGRAM_SESSION_NAME=
TARGET_CHANNEL_LINK=

# Необязательные (переопределяют значения по умолчанию)
# Имя файла сессии (без .session)
TELEGRAM_SESSION_NAME=

# True - парсить после даты, False - последние N
PARSE_BY_DATE=

# Используется если PARSE_BY_DATE=True
START_DATE_STR=
DATE_FORMAT=

# True - локальный пояс, False - использовать TIME_ZONE_STR
AUTO_DETECT_TIMEZONE=

# Используется если AUTO_DETECT_TIMEZONE=False
TIME_ZONE_STR=

# Сколько последних сообщений парсить (если PARSE_BY_DATE=False)
MESSAGE_LIMIT=

# Куда сохранять JSON
OUTPUT_DIRECTORY=

# Сохранять ли результат
SAVE_TO_JSON=

# LLM API
GIGACHAT_API_KEY=


# === PostgreSQL Database Settings ===
# Имя сервиса Postgres в docker-compose.yml
DB_HOST=
DB_PORT=
DB_USER=
DB_PASSWORD=
DB_NAME=



# ... (твои существующие переменные) ...

# === Airflow Settings ===
# Пользователь и пароль для подключения к БД метаданных Airflow
# (Эти же значения будут использоваться для инициализации БД PostgreSQL)
AIRFLOW_DB_USER=
AIRFLOW_DB_PASSWORD=
AIRFLOW_DB_NAME=
AIRFLOW_DB_HOST=
AIRFLOW_DB_PORT=

# UID, под которым будут запускаться процессы Airflow, чтобы избежать проблем с правами на volumes
# Для Linux/macOS можно использовать `id -u`
# Для WSL можно оставить 50000 или посмотреть `id -u` внутри WSL
AIRFLOW_UID=

# Имя пользователя и пароль для входа в Airflow Web UI (будут созданы при инициализации)
# Эти переменные читаются официальным образом Airflow для создания админа при первом запуске
_AIRFLOW_WWW_USER_USERNAME=
_AIRFLOW_WWW_USER_PASSWORD=

# Фернет-ключ для шифрования подключений и переменных в Airflow
# Сгенерируй один раз и сохрани в .env. Не коммить его, если он настоящий.
# Можно сгенерировать через: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW__CORE__FERNET_KEY= # СГЕНЕРИРУЙ_И_ВСТАВЬ_СЮДА_В_ФАЙЛЕ_.env

# Исполнитель Airflow (для начала LocalExecutor хорош для простоты)
AIRFLOW__CORE__EXECUTOR=

# Папка с DAG-файлами внутри контейнеров Airflow
AIRFLOW__CORE__DAGS_FOLDER=

# Загружать ли примеры DAG-ов (полезно для первого знакомства)
AIRFLOW__CORE__LOAD_EXAMPLES=